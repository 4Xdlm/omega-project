{
  "name": "omega-plugin-sdk-delivery",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "gate:no-shadow": "powershell -File scripts/gate-no-shadow.ps1",
    "gate:no-todo": "cd packages/sovereign-engine && npx tsx scripts/gate-no-todo.ts",
    "gate:active": "cd packages/sovereign-engine && npx tsx scripts/gate-active.ts",
    "gate:all": "npm run gate:no-shadow && npm run gate:no-todo && npm run gate:active"
  },
  "devDependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "fast-json-stable-stringify": "^2.1.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.17"
  }
}
