{
  "name": "@omega/emotion-gate",
  "version": "0.1.0",
  "description": "OMEGA Emotion Gate - Emotional validation layer with passive validators and SSOT compliance",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./gate": {
      "types": "./dist/gate/index.d.ts",
      "import": "./dist/gate/index.js"
    },
    "./validators": {
      "types": "./dist/validators/index.d.ts",
      "import": "./dist/validators/index.js"
    },
    "./ledger": {
      "types": "./dist/ledger/index.d.ts",
      "import": "./dist/ledger/index.js"
    },
    "./policy": {
      "types": "./dist/policy/index.d.ts",
      "import": "./dist/policy/index.js"
    },
    "./metrics": {
      "types": "./dist/metrics/index.d.ts",
      "import": "./dist/metrics/index.js"
    },
    "./proof": {
      "types": "./dist/proof/index.d.ts",
      "import": "./dist/proof/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@omega/canon-kernel": "file:../canon-kernel"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "keywords": [
    "omega",
    "emotion-gate",
    "validation",
    "ssot",
    "deterministic",
    "emotional-analysis"
  ],
  "author": "OMEGA Project",
  "license": "MIT"
}
